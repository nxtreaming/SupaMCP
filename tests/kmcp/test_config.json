{
  "clientConfig": {
    "clientName": "test-client",
    "clientVersion": "1.0.0",
    "useServerManager": true,
    "requestTimeoutMs": 30000
  },
  "mcpServers": {
    "local": {
      "command": "mcp_server",
      "args": ["--tcp", "--port", "8080"],
      "env": {
        "MCP_DEBUG": "1"
      }
    },
    "remote": {
      "url": "http://localhost:8080"
    }
  },
  "toolAccessControl": {
    "defaultAllow": true,
    "allowedTools": ["echo", "calculator", "translator"],
    "disallowedTools": ["dangerous_tool", "system_tool"]
  },
  "profiles": {
    "activeProfile": "development",
    "profileList": {
      "development": {
        "servers": {
          "local-dev": {
            "command": "mcp_server",
            "args": ["--tcp", "--port", "8080"],
            "env": {
              "MCP_DEBUG": "1"
            }
          }
        }
      },
      "production": {
        "servers": {
          "remote-prod": {
            "url": "http://localhost:8080"
          }
        }
      }
    }
  },
  "registry": {
    "registryUrl": "http://localhost:8081",
    "cacheTtlSeconds": 300,
    "connectTimeoutMs": 5000,
    "requestTimeoutMs": 10000,
    "maxRetries": 3
  },
  "toolSDK": {
    "toolsDirectory": "./tools",
    "autoLoadTools": true,
    "maxConcurrentTools": 10,
    "resourceLimits": {
      "maxMemoryMB": 1024,
      "maxCpuPercent": 50,
      "maxExecutionTimeMs": 60000
    }
  }
}
